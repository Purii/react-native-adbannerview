{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "valid-jsdoc": 1,
    "camelcase": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 1,
    "no-console": 1,
    "global-require": 0,
    "max-len": 1,
    "import/no-unresolved": 1,
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "__DEV__": true,
  }
}